{
  "name": "React extension",
  "description": "this is a react extension",
  "version": "0.0.1",

  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://practiceapi.geeksforgeeks.org/*","https://chat.openai.com/"],
  "permissions": ["tabs", "alarms", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["chatScript.js"]
    }
  ]
}
